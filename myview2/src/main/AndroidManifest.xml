<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn.myview2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service.MyService">
            <intent-filter>
                <action android:name="com.learn.myview2.service.SERVICE" />
            </intent-filter>
        </service>

        <activity android:name=".activity.MoveImgActivity" />
        <activity android:name=".activity.FlowLayoutActivity" />
        <activity android:name=".activity.EventBusActivity" />
        <activity android:name=".activity.LoadingCircleActivity"></activity>
    </application>

</manifest>
